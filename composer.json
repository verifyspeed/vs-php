{
    "name": "verifyspeed/vs-php",
    "description": "Official PHP SDK for VerifySpeed API",
    "version": "1.0.2",
    "type": "library",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.9",
        "ext-json": "*",
        "giggsey/libphonenumber-for-php": "^8.13",
        "endroid/qr-code": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "VerifySpeed\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "config": {
        "archive": {
            "exclude": [
                "/vendor",
                "/.gitignore",
                "/.idea",
                "/.vscode",
                "/examples/",
                "examples/*",
                "composer.lock",
                ".phpunit.result.cache",
                "*.test.php",
                "phpunit.xml"
            ]
        }
    },
    "keywords": [
        "verifyspeed",
        "verify",
        "vs",
        "verification",
        "sms",
        "otp",
        "qr-code",
        "deep-link",
        "whatsapp",
        "telegram",
        "viber",
        "phone",
        "phone-number",
        "phone-verification",
        "phone-number-verification",
        "sms-verification",
        "otp-verification",
        "qr-code-verification"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "VerifySpeed",
            "email": "support@verifyspeed.com"
        }
    ]
}
